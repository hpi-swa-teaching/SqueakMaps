converting
toStringLatLong
	
	| string |
	
	string := self lat abs asString.
	self lat > 0
		ifTrue: [string := string , ' 째N, ']
		ifFalse: [string := string , ' 째S, '].
		
	string := string, self long abs asString.
	self long > 0
		ifTrue: [string :=string , ' 째E']
		ifFalse: [string := string , ' 째W'].
	
	^ string
	